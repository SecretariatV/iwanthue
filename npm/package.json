    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medialab/iwanthue/issues"
  },
  "homepage": "https://github.com/medialab/iwanthue#readme",
  "devDependencies": {
    "@yomguithereal/eslint-config": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.3",
    "lodash": "^4.17.15",
    "mocha": "^7.0.1"
  },
  "eslintConfig": {
    "extends": "@yomguithereal/eslint-config",
